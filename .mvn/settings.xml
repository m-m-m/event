<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <servers>
    <server>
      <id>deploy-releases</id>
      <username>${env.GITHUB_USERNAME}</username>
      <password>${env.GITHUB_TOKEN}</password>
    </server>
    <server>
      <id>deploy-snapshots</id>
      <username>${env.GITHUB_USERNAME}</username>
      <password>${env.GITHUB_TOKEN}</password>
    </server>
    <server>
      <id>github</id>
      <username>${env.GITHUB_USERNAME}</username>
      <password>${env.GITHUB_TOKEN}</password>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>mmm</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <mmm.deploy.release.repository>https://maven.pkg.github.com/m-m-m</mmm.deploy.release.repository>
        <mmm.deploy.snapshot.repository>https://maven.pkg.github.com/m-m-m</mmm.deploy.snapshot.repository>
      </properties>
      <repositories>
       <repository>
         <id>github</id>
         <name>GitHub Packages</name>
         <url>https://maven.pkg.github.com/m-m-m</url>
       </repository>
     </repositories>
    </profile>
  </profiles>
</settings>
